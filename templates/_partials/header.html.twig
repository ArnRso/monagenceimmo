<header class="">
    <div class="left-container">
        <a href="{{ path('index') }}">
            <div class="logo-container">
                <img src="{{ asset('assets/img/logo_blanc.png') }}" alt="" class="img-fluid">
            </div>
        </a>
        <nav>
            <a href="{{ path('bien_index') }}">Trouver une location</a>
            <a href="{{ path('bien_new') }}">Déposer mon annonce</a>
            <a href="{{ path('favoris') }}">Mes Favoris</a>
        </nav>

    </div>

    <div class="profil">
        {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
            <a href="{{ path('profil') }}" class="btn rouge">Mon profil</a>
            <a href="{{ path('app_logout') }}" class="btn blanc">Se déconnecter</a>
        {% else %}
            <a href="{{ path('app_login') }}" class="btn rouge">Se connecter</a>
            <a href="{{ path('app_register') }}" class="btn blanc">S'inscrire</a>
        {% endif %}
    </div>
</header>